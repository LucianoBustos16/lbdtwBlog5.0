---
import BrandLogo from "@/components/BrandLogo.astro";

interface Props {
    class?: string;
  }
---



<BrandLogo class="dark:text-white text-primary w-32 animate-fade-in-up" />

<style>
    #logo{
        fill-opacity: 0;
        stroke-dasharray: 1300;
        stroke-dashoffset: 1300;
        animation: dash 3s ease-in forwards;
    }

    @keyframes dash {
        70% {
            fill-opacity: 0;
        }

        100%{
            fill-opacity: 100%;
            stroke-dashoffset: 0;
            stroke-width: 1;
        }
        
    }
</style>